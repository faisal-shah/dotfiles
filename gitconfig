[user]

[http]

[https]

[alias]
    addm = add -u

    br = branch
    ba = branch -av
    bd = branch -d
    bD = branch -D

    ci = commit
    cia = commit --amend

    f = fetch -p

    m = merge
    ma = merge --abort
    mc = merge --continue

    co = checkout
    cob = checkout -b

    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

    la = "!git config -l | sed -r -n 's/alias.//p'"

    p = push
    po = push origin
    puo = push -u origin

    re = reset
    rh = reset HEAD
    reh = reset --hard
    rem = reset --mixed
    res = reset --soft
    rehh = reset --hard HEAD
    remh = reset --mixed HEAD
    resh = reset --soft HEAD

    s = status

[credential]
    helper = store
